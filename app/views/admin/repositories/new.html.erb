<h1>Administration</h1>
<h2>Import</h2>
<div class="content row">
  <div class="dashboard-6 col-md-6">
    <h3><span class="glyphicon glyphicon-hdd"></span> Repositories</h3>
    <p>Select a file to import repositories.</p>
    <ul class="list-group">
      <% @repository_files.each do |file| %>
        <li class="list-group-item">
          <span class="badge"><%= repository_count(file) %></span>
          <%= file[:name] %>
        </li>
      <% end %>
    </ul>
  </div>
  <div class="dashboard-6 col-md-6">
    <h3><span class="glyphicon glyphicon-align-justify"></span> Metadata</h3>
  </div>
</div>

<h1><%= yield :heading %></h1>
<ol class="report repository breadcrumb">
  <li class="report repository selector">
    <a data-toggle="dropdown" href="#"><%= @repository.name == '*' ? 'All' : @repository.name %></a>
    <ul class="dropdown-menu" role="menu">
      <li><a href="?<%= yield :parameter %>=*">All</a></li>
      <li class="divider"></li>
      <% @repositories.each do |repository| %>
        <li><%= create_repository_link(repository) %></li>
      <% end %>
    </ul>
    <strong class="caret"></strong>
    <li class="active">Report</li>
  </li>
</ol>
<%= yield :content_top %>
<div class="dashboard">
  <%= yield :content %>
</div>
